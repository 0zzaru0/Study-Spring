<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hong" class="com.test03.Developer">
		<constructor-arg index="0" value="홍길동"></constructor-arg>
		<constructor-arg index="1" value="2000000"></constructor-arg>
		<property name="dept" value="개발"></property>
	</bean>
	
	<bean id="kim" class="com.test03.Engineer">
		<constructor-arg index="0" value="김나현"></constructor-arg>
		<constructor-arg index="1" value="2000000"></constructor-arg>
		<property name="dept" value="기술"/>
	</bean>

</beans>
